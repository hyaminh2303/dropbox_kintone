    "lint": "eslint src"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^1.12.2",
    "core-js": "^3.10.1",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.13.3",
    "@babel/preset-typescript": "^7.13.0",
    "@cybozu/eslint-config": "^12.0.1",
    "@kintone/dts-gen": "^3.1.9",
    "@kintone/plugin-uploader": "^4.3.0",
    "@kintone/webpack-plugin-kintone-plugin": "^4.0.19",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0"
